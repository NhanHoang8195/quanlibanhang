	<subDataset name="OrderCustomerByDay" uuid="0cc1d533-a0ca-49af-8b04-3a888957023d">
		<parameter name="dsAccountId" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select date_trunc('day', to_timestamp(date_order, 'MM/DD/YYYY, hh:mi:ss')::timestamp without time zone) as date, count(product_order_id) as sum, sum(total_money) as total_money
from product_order
where account_id = $P{dsAccountId}
GROUP BY date
Order by date;]]>
		</queryString>
		<field name="date" class="java.sql.Timestamp"/>
		<field name="sum" class="java.lang.Long"/>
		<field name="total_money" class="java.lang.Long"/>
	</subDataset>
